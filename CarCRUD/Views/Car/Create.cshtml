@model CarViewModel

<form asp-controller="Car" asp-action="Create" method="post">
	<input type="text" asp-for="Vin" readonly/>
	<input type="text" asp-for="Vendor" required="required" placeholder="Vendor"/>
	<input type="text" asp-for="Model" required="required" placeholder="Model" />
	<input type="submit" value="Create"/>
</form>
